<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="492.0" prefWidth="526.0" spacing="10" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ClubSearchController">
    <Label prefHeight="88.0" prefWidth="261.0" style="-fx-font-size: 24px; -fx-font-weight: bold;" text=" Club Searching Option" textAlignment="CENTER">
        <font>
            <Font name="Academy Engraved LET Plain:1.0" size="22.0" />
        </font>
        <VBox.margin>
            <Insets top="-50.0" />
        </VBox.margin>
        <padding>
            <Insets left="10.0" />
        </padding></Label>
    <Button onAction="#onMaximumSalary" prefHeight="34.0" prefWidth="290.0" text="Player(s) with the maximum salary of a club">
        <VBox.margin>
            <Insets bottom="15.0" />
        </VBox.margin></Button>
    <Button onAction="#onMaximumAge" prefHeight="34.0" prefWidth="288.0" text="Player(s) with the maximum age of a club">
        <VBox.margin>
            <Insets bottom="15.0" />
        </VBox.margin></Button>
    <Button onAction="#onMaximumHeight" prefHeight="34.0" prefWidth="274.0" text="Player(s) with the maximum height of a club">
        <VBox.margin>
            <Insets bottom="15.0" />
        </VBox.margin></Button>
    <Button onAction="#onTotalYearlySalary" prefHeight="34.0" prefWidth="206.0" text="Total yearly salary of a club">
        <VBox.margin>
            <Insets bottom="15.0" />
        </VBox.margin></Button>
    <Button onAction="#onBack" prefHeight="34.0" text="Back to Main Menu" />
</VBox>
